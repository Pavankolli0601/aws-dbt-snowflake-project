# Copy this file to profiles.yml and fill in your Snowflake credentials.
# Never commit profiles.yml to version control.

aws_dbt_snowflake_project:
  outputs:
    dev:
      account: your_snowflake_account
      database: AIRBNB
      password: "{{ env_var('SNOWFLAKE_PASSWORD') }}"  # Or set your password here (not recommended)
      role: ACCOUNTADMIN
      schema: dbt_schema
      threads: 1
      type: snowflake
      user: your_username
      warehouse: COMPUTE_WH
  target: dev
